# Git LFS configuration for image assets
src/assets/images/**/*.png filter=lfs diff=lfs merge=lfs -text
src/assets/images/**/*.jpg filter=lfs diff=lfs merge=lfs -text
src/assets/images/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
public/**/*.png filter=lfs diff=lfs merge=lfs -text
public/**/*.jpg filter=lfs diff=lfs merge=lfs -text
public/**/*.jpeg filter=lfs diff=lfs merge=lfs -text

