---
interface SocialLink {
    name: string;
    url: string;
}

const socialLinks: SocialLink[] = [];
---

<div class="flex flex-wrap gap-1">
    {
        socialLinks.map((link) => (
            <a
                href={link.url}
                target="_blank"
                rel="noopener noreferrer"
                class="border border-text-primary-dark px-[7px] py-[5px]"
            />
        ))
    }
</div>
