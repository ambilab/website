{
  "$schema": "https://unpkg.com/knip@5.85.0/schema.json",
  "entry": ["src/pages/**/*.{astro,ts}"],
  "project": ["src/**/*.{ts,tsx,astro,svelte}"],
  "ignore": [
    "src/components/astro/Card.astro",
    "src/components/svelte/DemoEmbed.svelte",
    "src/components/svelte/ResponsiveImage.svelte",
    "src/lib/images.ts"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreDependencies": ["tailwindcss-debug-screens"],
  "astro": {
    "config": ["astro.config.ts"],
    "entry": ["src/pages/**/*.astro"]
  },
  "typescript": {
    "config": ["tsconfig.json"]
  }
}
